    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^5.5.0",
    "protractor": "^5.4.2",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "0.37.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "3.5.3"
